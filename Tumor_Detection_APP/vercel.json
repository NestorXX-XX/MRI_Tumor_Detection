{
    "version": 2,
    "builds": [
        {
            "src": "Tumor_Detection_APP/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Tumor_Detection_APP/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "Tumor_Detection_APP.settings",
        "DJANGO_DEBUG": "False"
    }
} 